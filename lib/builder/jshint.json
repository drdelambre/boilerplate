{
	"curly": true,
	"eqeqeq": true,
	"forin": false,
	"freeze": true,
	"immed": true,
	"indent": 4,
	"latedef": true,
	"noarg": true,
	"nonbsp": true,
	"plusplus": false,
	"undef": true,
	"unused": true,
	"trailing": true,
	"maxparams": 4,
	"eqnull": true,
	"browser": true,
	"devel": false,
	"phantom": true,
	"sub": true,

	"globals": {
		// AMD
		"module":     true,
		"require":    true,
		"define":     true,

		// Environments
		"console":    true,
		"process":    true,

		// Node
		"__dirname":  true,
		"__filename": true
	}
}
